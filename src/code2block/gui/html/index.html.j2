<html>
<head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>

    <!-- Blockly -->
    <script src="../js/lib/blockly/blockly_compressed.js" type="text/javascript"></script>
    <script src="../js/lib/blockly/blocks_compressed.js" type="text/javascript"></script>
    <script src="../js/lib/blockly/msg/js/en.js" type="text/javascript"></script>
    <script src="../js/lib/blockly/python_compressed.js" type="text/javascript"></script>

    <!-- CodeMirror -->
    <link rel="stylesheet" href="../js/lib/codemirror/codemirror.css">
    <link rel="stylesheet" href="../js/lib/codemirror/fullscreen.css">
    <link rel="stylesheet" href="../js/lib/codemirror/show-hint.css">
    <script src="../js/lib/codemirror/codemirror.js" type="text/javascript"></script>
    <script src="../js/lib/codemirror/show-hint.js" type="text/javascript"></script>
    <script src="../js/lib/codemirror/python-hint.js" type="text/javascript"></script>
    <script src="../js/lib/codemirror/fullscreen.js" type="text/javascript"></script>
    <script src="../js/lib/codemirror/python.js" type="text/javascript"></script>

    <!-- Skulpt -->
    <script src="../js/lib/skulpt/dist/skulpt.js" type="text/javascript"></script>
    <script src="../js/lib/skulpt/dist/skulpt-stdlib.js" type="text/javascript"></script>

    <!-- BlockMirror -->
    <link rel="stylesheet" href="../js/lib/block_mirror/block_mirror.css">
    <script src="../js/lib/block_mirror/block_mirror.js" type="text/javascript"></script>

</head>
<body>
<div class="container">
    <br/>
    <br/>
    <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#import_module_modal">Import python module</button>
    <br/>
    <br/>
    <div class="row">
        <div id="blockmirror-editor" class="col-12" style="height: 700px"></div>
    </div>
</div>
<div class="modal fade" id="import_module_modal" data-bs-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content">
            <form method="post" id="import_module_modal_form" action="/get_blocks">
                <div class="modal-header">
                    <h5 class="modal-title">Import module</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body" id="import_module_modal_body">
                    <label for="tb_module_name">Module name:</label><br/>
                    <input type="text" name = "module_name" class="form-control" id = "tb_module_name"/><br/>
                </div>
                <div class="modal-footer">            
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal"> Cancel </button>
                    <button type="submit" class="btn btn-primary" id="btn_add_module_submit">
                        <span>Add module</span>
                        <span id="spinner_btn_add_module_submit" class="spinner-border spinner-border-sm" role="status" aria-hidden="true" hidden="false"></span-->
                    </button>     
                </div>
            </form>
        </div>
    </div>
</div>
</body>
</html>
<script type="text/javascript" src="../js/func.js"></script>
<script type="text/javascript" src="../js/index.js"></script>
